cb2017mod$rand1mod[cb2017mod$rand1mod==4] <- 5
cb2017mod$rand1mod[cb2017mod$rand1mod==3] <- 4
cb2017mod$rand1mod[cb2017mod$rand1mod==-1] <- 3
cb2017mod$rand1mod[cb2017mod$rand1mod==-2] <- 3
